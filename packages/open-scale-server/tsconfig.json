{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "./build",
        "baseUrl": "."
    },
    "exclude": [
        "node_modules",
        "build",
        "**/*.test.ts",
        "tester"
    ]
}
